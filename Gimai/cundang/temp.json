{
    "injector": {
        "enable": true,
        "print_loaded_modules": false
    },
    "font_manager": {
        "enable": true,
        "resource_files": [
            "MSYHBD.TTC"
        ],
        "spoof_enumeration": {
            "enable": true,
            "filter_pitch_and_family": 0,
            "filter_charset": 1,
            "spoof_charset": 128
        },
        "spoof_creation": {
            "enable": true,
            "override_face": "Microsoft Yahei UI Bold",
            "override_charset": 0
        }
    },
    "window_manager": {
    "enable": true,
    "overwrite_title": "My Window Title",
    "process_existing_windows": true
  }
}